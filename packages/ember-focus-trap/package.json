{
  "name": "ember-focus-trap",
  "version": "0.7.0",
  "description": "A Ember modifier to trap your focus.",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://josemarluedke.github.io/ember-focus-trap",
  "repository": "https://github.com/josemarluedke/ember-focus-trap",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "start": "rollup --config --watch",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@embroider/addon-shim": "^0.47.2",
    "@embroider/macros": "^0.47.2",
    "ember-auto-import": "^2.2.4",
    "ember-modifier-manager-polyfill": "^1.2.0",
    "focus-trap": "^6.7.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-proposal-decorators": "7.16.0",
    "@babel/plugin-syntax-decorators": "7.16.0",
    "@embroider/addon-dev": "0.47.2",
    "@rollup/plugin-babel": "5.3.0",
    "babel-eslint": "10.1.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.60.0"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./modifiers/focus-trap.js": "./dist/_app_/modifiers/focus-trap.js"
    }
  },
  "exports": {
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  }
}
